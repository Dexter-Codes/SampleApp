<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:local="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical"
	>
 <com.google.android.material.card.MaterialCardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/imgbtn_onboard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"       
        app:cardCornerRadius="17.38px"
        android:layout_marginLeft="5px"
        android:layout_marginRight="5px"
        android:layout_marginBottom="5px"
        android:layout_marginTop="30px"
        app:cardElevation="2dp"
        app:strokeWidth="1px"
        app:strokeColor="@color/card_border">
   
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="20px">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="5dp"
                android:weightSum="2"
                >
				<TextView
					android:text="12952"
					android:id="@+id/text_clientnum"
                    android:textSize="@dimen/text_size"
                    android:textColor="@color/text_bold"
                    android:fontFamily="@font/poppins_semibold"
                    android:layout_weight="1"
					android:layout_height="wrap_content"
					android:layout_width="0dp"
                    />
             
            <LinearLayout
        android:id="@+id/linearLayout1"
        android:orientation="horizontal"       
        android:layout_width="0dp"
        android:layout_height="55px"
        android:layout_marginLeft="82px"
        android:layout_marginRight="82px"
        android:layout_weight="0.5"
        android:background="@drawable/rounded_statusview">
        <TextView
            android:text="Completed"
            android:id="@+id/text_status"
            android:textSize="@dimen/text_sizeSmall"
            android:textColor="@android:color/white"
            android:fontFamily="@font/poppins_regular"
            android:gravity="center"
            android:textAlignment="center"
            android:layout_width="wrap_content"
            android:paddingLeft="5px"
            android:paddingTop="8px"
            android:layout_weight="3"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_width="29px"
            android:layout_height="29px"
            android:layout_gravity="center"
            android:layout_marginTop="5px"
            android:layout_marginBottom="5px"
            android:layout_marginRight="5px"
            android:layout_weight="1"
            android:src="@drawable/checkmark"/>
          </LinearLayout>
                </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="5dp"
                android:weightSum="1.8">
            <TextView
                    android:text="Client1"
                    android:id="@+id/text_clientname"
                    android:textSize="@dimen/text_size"
                    android:textColor="@color/text_bold"
                    android:fontFamily="@font/poppins_regular"
                    android:layout_weight="0.96"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    />
			<TextView
				android:text="22/12/2020"
				android:id="@+id/text_date"
				android:textSize="@dimen/text_size"
                android:textColor="@color/text_bold"
                android:fontFamily="@font/poppins_regular"
				android:layout_weight="0.55"
				android:layout_height="wrap_content"
				android:layout_width="0dp"
                    />
               <ImageView
                android:layout_width="77px"
                android:layout_height="77px"
                android:layout_marginBottom="20px"
                android:layout_weight="0.2"
                android:src="@drawable/next"/>
                </LinearLayout>
        <!--<LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-80px"
            android:layout_marginRight="40px"
            android:gravity="end">
        
            </LinearLayout>-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="5dp"
                android:weightSum="2">
            <TextView
                    android:text="PropertyID-12"
                    android:id="@+id/text_propid"
                    android:textSize="@dimen/text_sizeMedium"
                    android:fontFamily="@font/poppins_regular"
                    android:textColor="@color/text_bold"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    />
                 <TextView
                    android:text="67457"
                    android:id="@+id/text_propnum"
                    android:textSize="@dimen/text_sizeMedium"
                    android:fontFamily="@font/poppins_regular"
                    android:textColor="@color/text_bold"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    />
                </LinearLayout>
        </LinearLayout>
</com.google.android.material.card.MaterialCardView>
</RelativeLayout>